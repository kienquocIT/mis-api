#
msgid "Enter a valid \"slug\" consisting of letters, numbers, underscores or hyphens"
msgstr "Nhập một \"slug\" hợp lệ bao gồm các chữ cái, số, dấu gạch dưới hoặc dấu gạch nối"

#
msgid "The user does not belong to this company."
msgstr "Người dùng không thuộc về công ty này."

#
msgid "Please go to the tenant management page feature to add a company for this user."
msgstr "Vui lòng truy cập vào quản trị tenant để thêm công ty cho người dùng này."

#
msgid "Phone number should be a Vietnam phone number"
msgstr "Số điện thoại phải là số điện thoại Việt Nam"

#
msgid "2FA has been integrated"
msgstr "2FA đã được tích hợp"

#
msgid "Generate 2FA is failure"
msgstr "Tạo 2FA thất bại"

#
msgid "2FA verify is successful and turned on"
msgstr "Xác thực 2FA thành công và đã được bật"

#
msgid "2FA verify is failure"
msgstr "Xác thực 2FA thất bại"

#
msgid "2FA hasn't integrated"
msgstr "2FA chưa được tích hợp"

#
msgid "2FA was enabled"
msgstr "2FA đã kích hoạt"

#
msgid "2FA was disabled"
msgstr "2FA đã huỷ kích hoạt"

#
msgid "Your account has been locked due to too many incorrect OTP attempts."
msgstr "Tài khoản của bạn đã bị khóa do quá nhiều lần nhập sai mã OTP."

#
msgid "This OTP has already been used."
msgstr "Mã OTP này đã được sử dụng."

#
msgid "Deactivate 2FA before performing deletion"
msgstr "Vô hiệu hóa 2FA trước khi thực hiện xóa"

#
msgid "The refresh token must be required"
msgstr "Mã làm mới là bắt buộc"

#
msgid "The format day is incorrect. The format should be year[yyyy]-month[mm]-day[dd]"
msgstr "Định dạng ngày không chính xác. Định dạng nên là năm[yyyy]-tháng[mm]-ngày[dd]"
